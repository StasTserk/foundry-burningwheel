{
    "name": "burningwheel",
    "version": "1.5.7",
    "description": "Foundry VTT Implementation for The Burning Wheel",
    "main": "index.js",
    "scripts": {
        "build": "webpack --config webpack.config.js --mode=production",
        "build-dev": "webpack --config webpack.config.js --mode=development",
        "watch": "webpack --config webpack.config.js --mode=development --watch",
        "prettier": "npx prettier --write .",
        "docker-init": "webpack --config webpack.config.js --mode=production --env e2e && node ./tests/setup/setup.mjs",
        "p": "npx playwright test",
        "playwright": "webpack --config webpack.config.js --mode=production --env e2e && node ./tests/setup/setup.mjs && npx playwright test",
        "pu": "npx playwright test --ui",
        "playwright-ui": "webpack --config webpack.config.js --mode=production --env e2e && npx playwright test --ui"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@league-of-foundry-developers/foundry-vtt-types": "^12.331.3-beta",
        "@playwright/test": "^1.49.1",
        "@types/node": "^22.10.2",
        "@types/select2": "^4.0.57",
        "@typescript-eslint/eslint-plugin": "^5.60.0",
        "@typescript-eslint/parser": "^5.60.0",
        "cheerio": "^1.0.0",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.8.1",
        "eslint": "^8.43.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsdoc": "^46.2.6",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-webpack-plugin": "^4.0.1",
        "fetch-cookie": "^3.0.1",
        "file-loader": "^6.2.0",
        "handlebars": "^4.7.7",
        "js-yaml": "^4.1.0",
        "mini-css-extract-plugin": "^2.7.6",
        "node-fetch": "^3.3.2",
        "prettier": "^2.8.8",
        "sass": "^1.63.6",
        "sass-loader": "^13.3.2",
        "style-loader": "^3.3.3",
        "testcontainers": "^10.16.0",
        "ts-loader": "^9.4.3",
        "typescript": "^5.1.3",
        "webpack": "^5.94.0",
        "webpack-cli": "^5.1.4",
        "webpack-prettier-plugin": "^1.0.3",
        "yaml-loader": "^0.8.0"
    },
    "dependencies": {
        "select2": "^4.0.13"
    }
}
